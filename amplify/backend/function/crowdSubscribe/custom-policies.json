[
  {
    "Effect": "Allow",
    "Action": [
      "sns:*"
    ],
    "Resource": ["*"]
  },
  {
    "Effect": "Allow",
    "Action": [
        "kms:ListAliases",
        "iam:GetPolicy",
        "iam:GetPolicyVersion",
        "iam:GetRole",
        "iam:GetRolePolicy",
        "iam:ListAttachedRolePolicies",
        "iam:ListRolePolicies",
        "iam:ListRoles",
        "lambda:*",
        "tag:GetResources",
        "xray:GetTraceSummaries",
        "xray:BatchGetTraces"
    ],
    "Resource": ["*"]
  },
  {
    "Effect": "Allow",
    "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
    ],
    "Resource": ["*"]
  },
  {
    "Effect": "Allow",
    "Action":["lambda:InvokeFunction"],
    "Resource": ["*"]
  }
]
